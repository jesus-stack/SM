@model List<Infraestructure.Models.Salida>


<div class="row container table-responsive col-12">
    <table class="table  table-striped table-bordered text-center">
        <thead class="bg-sprimary">
            <tr>

                <th>N Entradas</th>
                <th>Usuario</th>
                <th>Movmiento</th>
                <th>Fecha</th>


            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.Id)</td>
                    <td>@Html.DisplayFor(modelItem => item.Usuario.Nombre)</td>
                    <td>@Html.DisplayFor(modelItem => item.Movimiento.Nombre)</td>
                    <td>@Html.DisplayFor(modelItem => item.fecha)</td>
                </tr>

            }
        </tbody>
    </table>



</div>

